{
    "swagger": "2.0",
    "info": {
        "description": "API Server for ComputerClub Application",
        "title": "ComputerClub API",
        "contact": {},
        "version": "1.02"
    },
    "host": "localhost:8181",
    "basePath": "/",
    "paths": {
        "/computers": {
            "post": {
                "description": "Создает запись о новом компьютере в базе данных",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "computers"
                ],
                "summary": "Создать новый компьютер",
                "parameters": [
                    {
                        "description": "Данные компьютера",
                        "name": "computer",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Computer"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Computer created successfully",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "400": {
                        "description": "error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/computers/available": {
            "get": {
                "description": "Возвращает список всех доступных (свободных) компьютеров",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "computers"
                ],
                "summary": "Получить список доступных компьютеров",
                "responses": {
                    "200": {
                        "description": "Список доступных компьютеров",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Computer"
                            }
                        }
                    },
                    "400": {
                        "description": "error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Computer": {
            "type": "object",
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "deleted_at": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "is_available": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                },
                "updated_at": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "ApiKeyAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}